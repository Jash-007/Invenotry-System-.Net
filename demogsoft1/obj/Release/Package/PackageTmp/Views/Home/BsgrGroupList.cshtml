@model IEnumerable<demogsoft1.Models.mstBsgrGroup>

@{
    ViewBag.Title = "BsgrGroupList";
}

<h2>BsgrGroupList</h2>

<p>
    @Html.ActionLink("Create New Group", "AddBsgrGroup","Home",new {@class="btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GroupName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GroupName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "Delete", new { id=item.BsgrId })
        </td>
    </tr>
}

</table>
