@model IEnumerable<demogsoft1.Models.mstTax>

@{
    ViewBag.Title = "TaxTypeList";
}

<h2>TaxTypeList</h2>

<p>
    @Html.ActionLink("Create New", "AddTaxType","Home",new {@class="btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TaxName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TaxPrc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SGSTPrc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CGSTPrc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IGSTPrc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TaxName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TaxPrc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SGSTPrc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CGSTPrc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IGSTPrc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditTaxType", new { id = item.TaxId },new {@class="btn btn-success"}) |
           
            @Html.ActionLink("Delete", "DeleteTax", new { id = item.TaxId },new {@class="btn btn-danger"})
        </td>
    </tr>
}

</table>
