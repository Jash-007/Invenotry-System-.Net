@model IEnumerable<demogsoft1.Models.mstCompany>

@{
    ViewBag.Title = "CompanyList";
}

<h2>CompanyList</h2>

<p>
    @Html.ActionLink("Create New", "AddCompany","Home",new {@class="btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CompanyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mstState.StateName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mstState.StateName)
        </td>
        <td>
           
        </td>
    </tr>
}

</table>
